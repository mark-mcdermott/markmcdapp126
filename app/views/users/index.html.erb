<p style="color: green"><%= notice %></p>

<h1>Users</h1>

<div id="users">

  <!-- <%= link_to "New user", new_user_path, class: 'btn btn-primary' %> -->

  <% @users.each do |user| %>
    <%= render user %>
    <p>
      <%= link_to "Show this user", user, class: 'btn btn-secondary btn-sm my-2' %>
    </p>
  <% end %>
</div>
